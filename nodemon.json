{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "ts": "node --require ts-node/register"
  },
  "watch": [
    "src/"
  ],
  "env": {
    "OPENAI_KEY": "OPENAI_KEY",
    "TELEGRAM_TOKEN": "TELEGRAM_TOKEN",
    "MONGO_USER": "MONGO_USER",
    "MONGO_PASSWORD": "MONGO_PASSWORD"
  },
  "ext": "js,json,ts"
}
